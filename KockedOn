Main.Name = "Main"
Main.Parent = ScreenGui
Main.BackgroundColor3 = Color3.fromRGB(63, 63, 63)
Main.Position = UDim2.new(0.0426917411, 0, 0.371359229, 0)
Main.Size = UDim2.new(0, 157, 0, 61)
Main.Active = true
Main.Draggable = true

AntiLagOn.Name = "AntiLagOn"
AntiLagOn.Parent = Main
AntiLagOn.BackgroundColor3 = Color3.fromRGB(170, 0, 0)
AntiLagOn.Position = UDim2.new(0.028806584, 0, 0.229508191, 0)
AntiLagOn.Size = UDim2.new(0, 58, 0, 32)
AntiLagOn.Font = Enum.Font.SourceSans
AntiLagOn.Text = "PushEveryone"
AntiLagOn.TextColor3 = Color3.fromRGB(0, 0, 0)
AntiLagOn.TextSize = 14.000
AntiLagOn.MouseButton1Down:connect(function()
	

local Player = game:GetService("Players").LocalPlayer
local client = Player.PlayerGui.autoExec.client
local DrawLaserF = nil

for I, V in pairs(getgc()) do
    if type(V) == "function" and getfenv(V).script == client and debug.getinfo(V).name == "drawLaser" then
        DrawLaserF = V
        break 
    end
end
function DrawLaser(...)
   DrawLaserF(...)
   game:GetService("Workspace").resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", ...)
end
local Pos = Player.Character.Torso.Position
DrawLaser(Pos, Vector3.new(Pos.X, Pos.Y+100, Pos.Z), {
    ["BrickColor"] = BrickColor.new("Really black"),
["Shape"] = Enum.PartType.Ball,
["Position"] = Pos,
["Material"] = Enum.Material.ForceField,
["Size"] = Vector3.new(120, 120, 50),
["CanCollide"] = false,
["Name"] = "Part87"
})





local p = game.Players.LocalPlayer.Character.Torso.Position
local tbl = {
    ["CanCollide"] = true,
    ["Parent"] = workspace,
    ["Anchored"] = true,
    ["Name"] = "Part",
    ["CastShadow"] = false,
    ["Transparency"] = 0,
    ["RotVelocity"] = Vector3.new(0,0,0),
    ["Position"] = p,
    ["BrickColor"] = BrickColor.new("Really black"),
    ["Material"] = Enum.Material.ForceField,
    ["Shape"] = Enum.PartType.Ball,
    ["Size"] = Vector3.new(120, 50, 120)
}

workspace.resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", p, p, tbl)
    


end)


Stand.Name = "Stand"
Stand.Parent = Main
Stand.BackgroundColor3 = Color3.fromRGB(170, 0, 0)
Stand.Position = UDim2.new(0.551994443, 0, 0.229508191, 0)
