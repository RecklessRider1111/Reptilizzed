    WeaponsSection:NewToggle("Pizza", "", function(state)
        if state then
           
            local plr = game.Players.LocalPlayer


            local pt = Instance.new("Part")
             pt.Parent = plr.Character
            pt.Anchored = true
            pt.CanCollide= false
            pt.Name = "FixedTP"
            pt.CFrame = plr.Character.HumanoidRootPart.CFrame
            pt.Transparency = 1
            
            
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(48.8090401, 281.938049, 1400.03162, -0.611430407, 5.34529434e-08, 0.791298211, 1.56600265e-07, 1, 5.34529434e-08, -0.791298211, 1.56600265e-07, -0.611430407)
            wait(0.2)
            
            
            
            local args = { 
            [1] = "IRC",
            
            [2] = "Pizza", 
            [3] = true
            }
            game:GetService("ReplicatedStorage").GunShopEvent:FireServer(unpack(args))
            
            
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.FixedTP.CFrame
            game.Players.LocalPlayer.Character.FixedTP:Destroy()

        else
           
            local plr = game.Players.LocalPlayer


            local pt = Instance.new("Part")
             pt.Parent = plr.Character
            pt.Anchored = true
            pt.CanCollide= false
            pt.Name = "FixedTP"
            pt.CFrame = plr.Character.HumanoidRootPart.CFrame
            pt.Transparency = 1
            
            
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(48.8090401, 281.938049, 1400.03162, -0.611430407, 5.34529434e-08, 0.791298211, 1.56600265e-07, 1, 5.34529434e-08, -0.791298211, 1.56600265e-07, -0.611430407)
            wait(0.2)
            
            
            
            local args = { 
            [1] = "IRC",
            
            [2] = "Pizza", 
            [3] = true
            }
            game:GetService("ReplicatedStorage").GunShopEvent:FireServer(unpack(args))
            
            
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.FixedTP.CFrame
            game.Players.LocalPlayer.Character.FixedTP:Destroy()



        end
    end)
    

    


