    MainSection:NewButton("Click Arrest", "", function()
        
function CHECK()
    if game.Players.LocalPlayer.Character:FindFirstChild("Handcuffs") == nil then
    game.Players.LocalPlayer.Backpack.Handcuffs.Parent = game.Players.LocalPlayer.Character
    end
    end
    
    
    local plr = game.Players.LocalPlayer
    local mouse = plr:GetMouse()
    local tool = Instance.new("Tool", plr.Backpack)
    tool.Name = "Click Arrest"
    tool.CanBeDropped = false
    tool.RequiresHandle = false
    
    tool.Activated:connect(function()
        local t = mouse.Target
        local target = nil
        if t then
          for i, v in pairs(game.Players:GetPlayers()) do
            if v.Character and v.Character:FindFirstChild("UpperTorso") or
            v.Character and v.Character:FindFirstChild("LowerTorso") or 
            v.Character and v.Character:FindFirstChild("Head") or
            v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
            
                if t:IsDescendantOf(v.Character) then
                    target = v
                    break
                end
            end
          end
        end
        if target then --your code down here:
    
    
    CHECK()
    local pt = Instance.new("Part")
     pt.Parent = plr.Character
    pt.Anchored = true
    pt.CanCollide= false
    pt.Name = "FixedTP"
    pt.CFrame = plr.Character.HumanoidRootPart.CFrame
    pt.Transparency = 1
    
    
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame
    
    wait(0.2)
    local args = {
    
    [1] = target
    
    }
    game:GetService("ReplicatedStorage").Arrest:FireServer(unpack(args))
    wait(0.2)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.FixedTP.CFrame
    game.Players.LocalPlayer.Character.FixedTP:Destroy()
    game.Players.LocalPlayer.Character.Handcuffs.Parent = game.Players.LocalPlayer.Backpack
    
    end
    end)
    end)

    

