game.Players.LocalPlayer.HealthDisplayDistance = 500

local function LavaFE() 
    print("The Darkones AntiSpy Has Been Activated")
    
    function kick()
    game.Players.LocalPlayer:Kick("NOPE")
    end
    
    
    
    
    function antispy1()
    local model = game.CoreGui
    for i, v in pairs(model:GetDescendants()) do
    if v:FindFirstChild("ColorBar") then
    kick()
    end
    end
    end
    
    
    
    
    
    function antispy2()
    if game.CoreGui:FindFirstChild("SimpleSpy2") then
    kick()
    end
    end
    
    
    
    function antispy3()
    if game.Players.LocalPlayer.PlayerGui:FindFirstChild("gui") then
    kick()
    end
    end
    
    
    
    function antispy4()
    local model = game.CoreGui
    for i, v in pairs(model:GetDescendants()) do
    if v:FindFirstChild("ContextMenus") then
    kick()
    end
    end
    end
    
    
    
    
    function antispy5()
    if _G.scanRemotes == true then
    kick()
    end
    end
    
    
    
    
    
    
    
    
    
    
    while task.wait() do
    antispy1()
    antispy2()
    antispy3()
    antispy4()
    antispy5()
    
    
    
    
    end
    end
    
    
    local function LL() 
        
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("SWAT VS CRIMS ADMIN", "DarkTheme")
    --main
    
    local Main = Window:NewTab("Abilities")
    local MainSection = Main:NewSection("Abilities")

    MainSection:NewButton("Click Kill", "Kills On Click", function()
        local plr = game.Players.LocalPlayer
local mouse = plr:GetMouse()
local tool = Instance.new("Tool", plr.Backpack)
tool.Name = "Click Kill"
tool.CanBeDropped = false
tool.RequiresHandle = false

tool.Activated:connect(function()
    local t = mouse.Target
    local target = nil
    if t then
      for i, v in pairs(game.Players:GetPlayers()) do
        if v.Character and v.Character:FindFirstChild("UpperTorso") or
        v.Character and v.Character:FindFirstChild("LowerTorso") or 
        v.Character and v.Character:FindFirstChild("Head") or
        v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
        
            if t:IsDescendantOf(v.Character) then
                target = v
                break
            end
        end
      end
    end
    if target then --your code down here:

if game.Players.LocalPlayer.TeamColor == BrickColor.new("Really black") then

    local args = {
        [1] = target.Character.Humanoid, 
        [2] = target.Character.UpperTorso, 
        [3] = 1000.5,
        [4] = Vector3.new(0.829352080821991, -0.26517945528030396, -0.49178752303123474),
        [5] = 0,
        [6] = false, 
        [7] = false
        }
        game:GetService("Players").LocalPlayer.Backpack.Rifleswat.GunScript_Server.InflictTarget:FireServer(unpack(args))
        

else

local args = {
[1] = target.Character.Humanoid, 
[2] = target.Character.UpperTorso, 
[3] = 1000.5,
[4] = Vector3.new(0.829352080821991, -0.26517945528030396, -0.49178752303123474),
[5] = 0,
[6] = false, 
[7] = false
}
game:GetService("Players").LocalPlayer.Backpack.Riflecrim.GunScript_Server.InflictTarget:FireServer(unpack(args))
end
end
end)
    end)





    MainSection:NewButton("Click Damage", "Damages On Click", function()
        local plr = game.Players.LocalPlayer
local mouse = plr:GetMouse()
local tool = Instance.new("Tool", plr.Backpack)
