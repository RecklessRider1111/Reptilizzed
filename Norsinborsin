 local function FithPhase()
   wait(25)
    _G.Kill = true
    while _G.Kill == true do
        wait()
killall()

 end
end

    coroutine.wrap(FithPhase)()



    
 local function Last()
    wait(40)
    _G.Kill = false
    while _G.Kill == true do
        wait()

    end
 end


    coroutine.wrap(Last)()




    local function Last1()
        wait(40)
        
    game.Workspace.BlackHole:Destroy()
        end
     
    
    
        coroutine.wrap(Last1)()
