					["CastShadow"] = false,
					["Transparency"] = 0,
					["RotVelocity"] = Vector3.new(0,0,0),
					["Position"] = p,
					["BrickColor"] = x["Explosion3"].BrickColor,
					["Material"] = Enum.Material.Neon,
					["Shape"] = Enum.PartType.Ball,
					["Size"] = x["Explosion3"].Size,
				  ["Rotation"] = x["Explosion3"].Rotation,
				}
				
				workspace.resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", p, p,  tbl)
						 
						
				local p = x["Explosion4"].Position
				local tbl = {
					["CanCollide"] = true,
				  
					["Parent"] = workspace,
					["Anchored"] = true,
					["Name"] = "Part",
					["CastShadow"] = false,
					["Transparency"] = 0,
					["RotVelocity"] = Vector3.new(0,0,0),
					["Position"] = p,
					["BrickColor"] = x["Explosion4"].BrickColor,
					["Material"] = Enum.Material.Neon,
					["Shape"] = Enum.PartType.Ball,
					["Size"] = x["Explosion4"].Size,
				  ["Rotation"] = x["Explosion4"].Rotation,
				}
				
				workspace.resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", p, p,  tbl)
						 
						
						
			end
					end
			
			
					local function EndAll() 
			
						workspace:WaitForChild("Meteior")
						workspace.Meteior:WaitForChild("Explosion3")
						wait(20)
			
			workspace.Meteior:Destroy()
			
			wait(60)
			game.Workspace.MeteorStop.Name = "AllCars"
					end
			
			
				   
			  coroutine.wrap(Explosion)()
				 coroutine.wrap(MeteiorFalling)()
				 coroutine.wrap(MainExplosion)()
				 coroutine.wrap(EndAll)()
			end
			
		
		
		
		
		
		
		
		end)
end
