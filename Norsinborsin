 coroutine.wrap(ThirdPhase)()

 
 local function FourthPhase() -- ClickFling.LocalScript 
 
 
    game.Workspace:WaitForChild("BlackHole")
    game.Workspace.BlackHole:WaitForChild("Hole")
 
    wait(12)
 
    local TweenService = game:GetService("TweenService")
    local part = workspace.BlackHole.Hole
    local info = TweenInfo.new(10, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, -1, true)
    
    local tween = TweenService:Create(part,info, {CFrame = CFrame.new(-223.886063, 91, -250.543823, 1, 0, 0, 0, 1, 0, 0, 0, 1)} )
    
    tween:Play()
   
   wait(10)
game.Workspace.BlackHole.Hole:Destroy()
   local d  =  Instance.new("Part")
   d.Size = Vector3.new(2042, 2042, 2042)
   d.Parent = a
   d.Material = Enum.Material.SmoothPlastic 
   d.CFrame = CFrame.new(-223.886063, 91, -250.543823, 1, 0, 0, 0, 1, 0, 0, 0, 1)
   d.BrickColor = BrickColor.new("Really black")
   d.Transparency = 0
   d.CanCollide = true
   d.CanTouch = true
   d.Anchored = an
   d.Shape = Enum.PartType.Ball
    d.Name = "Hole2"
   



    game.Workspace:WaitForChild("BlackHole")
 game.Workspace.BlackHole:WaitForChild("Hole2")
 local z = game.Workspace.BlackHole.Hole2
 
 _G.Hole2 = true
 while _G.Hole2 == true do
     wait()
 
 if game.Workspace.BlackHole:FindFirstChild("Hole2") then
 
     
     local p = z.Position
     local tbl = {
         ["CanCollide"] = true,
       
         ["Parent"] = workspace,
         ["Anchored"] = true,
         ["Name"] = "Part",
         ["CastShadow"] = false,
         ["Transparency"] = 1,
         ["RotVelocity"] = Vector3.new(0,0,0),
         ["Position"] = p,
         ["BrickColor"] = z.BrickColor,
         ["Material"] = Enum.Material.SmoothPlastic,
         ["Shape"] = Enum.PartType.Ball,
         ["Size"] = z.Size,
       ["Rotation"] = z.Rotation,
 
     }
     
     workspace.resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", p, p,  tbl)
       
 
 else
     _G.Hole2 = false
     while _G.Hole2 == true do
         wait()
 
 end
 end
end
 end
 

 coroutine.wrap(FourthPhase)()



