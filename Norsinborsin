coroutine.wrap(VNQBASY_fake_script)()
local function TLZT_fake_script() -- BlackHole.LocalScript 
	local script = Instance.new('LocalScript', BlackHole)

	script.Parent.MouseButton1Down:connect(function()
		
		
		
		if game.Workspace:FindFirstChild("AllFoodTrays") then
			game.Workspace.AllFoodTrays.Name = "BlackWholeStop"
			
				  
				local word = "A black hole has been spotted"
				local lib = require(game:GetService("Workspace").upsilonLibrary)
				firesignal(game.Workspace.resources.RemoteEvent.OnClientEvent,"displayChoice",'returnToMenu',word)
				lib.FireOtherClients("displayChoice",'returnToMenu',word)
				  
				  wait(2)
				  
				  local word = "Make sure to take...#.#%%%..##%%%%%"
				local lib = require(game:GetService("Workspace").upsilonLibrary)
				firesignal(game.Workspace.resources.RemoteEvent.OnClientEvent,"displayChoice",'returnToMenu',word)
				lib.FireOtherClients("displayChoice",'returnToMenu',word)
			
			
				wait(5)
				 
				
				
				local a = Instance.new("Model")
					a.Name = "BlackHole"
					a.Parent = workspace
				
				local an = true
				  local am = false
			
			
			local function Start() -- ClickFling.LocalScript 
				
				   
					local b  =  Instance.new("Part")
					b.Size = Vector3.new(1750, 250, 1484)
					b.Parent = a
					b.Material = Enum.Material.ForceField 
					b.CFrame = CFrame.new(-128.380066, -1.5, -386.209351, 1, 0, 0, 0, 1, 0, 0, 0, 1)
					b.BrickColor = BrickColor.new("Dark stone grey")
					b.Transparency = 0
					b.CanCollide = false
					b.CanTouch = false
					b.Anchored = an
					b.Shape = Enum.PartType.Block
					 b.Name = "SuckPart"
					b.AssemblyLinearVelocity = Vector3.new(0, 0, -99)
				   
				game.Workspace:WaitForChild("BlackHole")
				game.Workspace.BlackHole:WaitForChild("SuckPart")
				local z = game.Workspace.BlackHole.SuckPart
				
				_G.BlackHole = true
				while _G.BlackHole == true do
					wait()
				
				if game.Workspace.BlackHole:FindFirstChild("SuckPart") then
				
					
					local p = z.Position
					local tbl = {
						["CanCollide"] = true,
					  
						["Parent"] = workspace,
						["Anchored"] = true,
						["Name"] = "Part",
						["CastShadow"] = false,
						["Transparency"] = 1,
						["RotVelocity"] = Vector3.new(0,0,0),
						["Position"] = p,
						["BrickColor"] = z.BrickColor,
						["Material"] = Enum.Material.ForceField,
						["Shape"] = Enum.PartType.Block,
						["Size"] = z.Size,
					  ["Rotation"] = z.Rotation,
				   ["AssemblyLinearVelocity"] = z.AssemblyLinearVelocity
					}
					
					workspace.resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", p, p,  tbl)
					  
				
				else
					_G.BlackHole = false
					while _G.BlackHole == true do
						wait()
				
				end
				end
				end
				end
				coroutine.wrap(Start)()
				
				
				
			
