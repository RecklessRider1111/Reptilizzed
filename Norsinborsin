local function CGGTJXJ_fake_script() -- SpamPlaySounds.LocalScript 
	local script = Instance.new('LocalScript', SpamPlaySounds)

	script.Parent.MouseButton1Down:connect(function()
	
	
local function a()
	while task.wait(0.3) do
   
   
   
   local v = workspace.ACR.Handle.FireSound
	   
	   
	   
	   local lib = require(game:GetService("Workspace").upsilonLibrary)
	   firesignal(game.Workspace.resources.RemoteEvent.OnClientEvent,"playSound",'playSound',v)
	   lib.FireOtherClients("playSound",'playSound',v)
   end
   end
   coroutine.wrap(a)()
   
   
   
   
   
   
   
   
   
   local function b()
   while task.wait(0.3) do
   
   
   local v = workspace.AK47.Handle.FireSound
	   
	   
	   
	   local lib = require(game:GetService("Workspace").upsilonLibrary)
	   firesignal(game.Workspace.resources.RemoteEvent.OnClientEvent,"playSound",'playSound',v)
	   lib.FireOtherClients("playSound",'playSound',v)
   end
   end
   
   coroutine.wrap(b)()
   
   
   local function c()
   
   while task.wait(0.3) do
   
   
   local v = workspace["AK47-U"].Handle.FireSound
	   
	   
	   
	   local lib = require(game:GetService("Workspace").upsilonLibrary)
	   firesignal(game.Workspace.resources.RemoteEvent.OnClientEvent,"playSound",'playSound',v)
	   lib.FireOtherClients("playSound",'playSound',v)
   end
   end
   
   coroutine.wrap(c)()
   
   
   local function d()
