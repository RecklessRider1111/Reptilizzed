        MainSection:NewButton("Click TP", "TPS On Click", function()
            local plr = game.Players.LocalPlayer
	local mouse = plr:GetMouse()
	local tool = Instance.new("Tool", plr.Backpack)
	tool.Name = "Click TP"
	tool.CanBeDropped = false
	tool.RequiresHandle = false
	tool.Activated:connect(function()


local pt = Instance.new("Part")
 pt.Parent = plr.Character
pt.Anchored = true
pt.CanCollide= false
pt.Name = "FixedTP"
pt.CFrame = plr.Character.HumanoidRootPart.CFrame

local pos = mouse.Hit+Vector3.new(0, 3, 0)
			pos = Vector3.new(pos.X, pos.Y, pos.Z)
game.Players.LocalPlayer.Character.FixedTP.Position = pos
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.FixedTP.CFrame
game.Players.LocalPlayer.Character.FixedTP:Destroy()
end)
        end)


        local Teams = Window:NewTab("Extra")
        local TeamsSection = Teams:NewSection("Extra")



        TeamsSection:NewButton("Crims Team", "Switches Your Team", function()
            local pt = Instance.new("Part")
            pt.Parent = game.Workspace
           pt.Anchored = true
           pt.CanCollide= false
           pt.Name = "TPPP"
           pt.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
           pt.Transparency = 1
           
           game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(126.408134, 215.739502, -242.241226, -0.141894996, 7.57889822e-08, -0.989881694, -2.21657626e-08, 1, 7.97410351e-08, 0.989881694, 3.32563381e-08, -0.141894996)
           local name = game.Players.LocalPlayer.Name
           
           
           
           wait(1.5)
           game.Workspace:WaitForChild(name)
           
           game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
           game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.TPPP.CFrame
           
           workspace.TPPP:Destroy()
        end)



        TeamsSection:NewButton("SWAT Team", "Switches Your Team", function()
            local pt = Instance.new("Part")
            pt.Parent = game.Workspace
           pt.Anchored = true
           pt.CanCollide= false
           pt.Name = "TPPP"
           pt.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
           pt.Transparency = 1
           
           game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(74.9969177, 215.739502, -242.706039, 0.98279959, -2.02111021e-08, -0.184675366, 2.33745148e-08, 1, 1.49525121e-08, 0.184675366, -1.90120186e-08, 0.98279959)
           
           
           wait(2.5)
           
           game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.TPPP.CFrame
           
           workspace.TPPP:Destroy()
        end)


