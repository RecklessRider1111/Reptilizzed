Character.MouseButton1Down:connect(function()



	local ch = game.Players.LocalPlayer.Character
	ch.HumanoidRootPart.CFrame = CFrame.new(-798.605835, -5.99868059, -426.678284, -0.759713292, -3.62488861e-09, -0.650258183, -1.08926179e-09, 1, -4.30192504e-09, 0.650258183, -2.55992827e-09, -0.759713292)
	wait(0.5)



	local hg = Instance.new("Part")
	hg.Anchored = true
