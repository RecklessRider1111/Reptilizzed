			local function ZndPartShow() -- TextButton.LocalScript 
				local script = Instance.new('LocalScript', workspace)
				local x = game.Workspace
				while task.wait() do
				
				if x:FindFirstChild("LavaPart2") then
				local p = x["LavaPart2"].Position
			  local tbl = {
				  ["CanCollide"] = true,
				
				  ["Parent"] = workspace,
				  ["Anchored"] = true,
				  ["Name"] = "Part",
				  ["CastShadow"] = false,
				  ["Transparency"] = 1,
				  ["RotVelocity"] = Vector3.new(0,0,0),
				  ["Position"] = p,
				  ["BrickColor"] = x["LavaPart2"].BrickColor,
				  ["Material"] = Enum.Material.Neon,
				  ["Shape"] = Enum.PartType.Block,
				  ["Size"] = x["LavaPart2"].Size,
				["Rotation"] = x["LavaPart2"].Rotation,
			  }
			  
			  workspace.resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", p, p,  tbl)
			
			end
			end
			end
			
			
				
			
