local function JTFHH_fake_script() -- DarkenTheMap.LocalScript 
	local script = Instance.new('LocalScript', DarkenTheMap)

	script.Parent.MouseButton1Down:connect(function()
	if script.Parent.BackgroundColor3 == Color3.fromRGB(255, 0, 0) then
			script.Parent.BackgroundColor3 = Color3.fromRGB(155, 0, 0)
	
	  
  
local a  =  Instance.new("Part")
a.Size = Vector3.new(2044, 964, 1895)
a.Parent = workspace
a.Material = Enum.Material.ForceField 
a.Position = Vector3.new(-111.01492309570312, -200.5, -373.37939453125)

a.Transparency = 0
a.CanCollide = false
a.CanTouch = false
a.Anchored = true
a.Shape = Enum.PartType.Block
 a.Name = "ShadowPart"
  
  
  
  
  
  
  
  _G.shadow = true
while _G.shadow == true do
    wait()
 

 local p = Vector3.new(-111.01492309570312, -200.5, -373.37939453125)
        local tbl = {
            ["CanCollide"] = false,
        ["Reflectance"] = Vector3.new(2044, 964, 1895),
            ["Parent"] = workspace,
            ["Anchored"] = true,
            ["Name"] = "Shadow",
            ["Transparency"] = 1,
            ["Position"] = p,
            ["BrickColor"] = BrickColor.new("Institutional white"),
            ["Material"] = Enum.Material.ForceField,
            ["Shape"] = Enum.PartType.Block,
            ["Size"] = Vector3.new(2044, 964, 1895),
            ["Rotation"] = Vector3.new(0, 0, 0)
        }
        
        workspace.resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", p, p, tbl)
      end
	
	
	
	else
			script.Parent.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
			
			
			workspace.ShadowPart:Destroy()
_G.shadow = false
while _G.shadow == true do
    wait()
end
end
	end)
end
