			local function Explosion() -- TextButton.LocalScript 
				local script = Instance.new('LocalScript', workspace)
			   
			
			while task.wait() do
			   if am:FindFirstChild("Explosion") then
			   if am.Explosion.Size.Y > 2000 then
					am.Explosion:Destroy()
			 am:Destroy()
			wait(45)
			game.Workspace.LaserStop.Name = "ignore"
			end
			end
			end
			end
			
			coroutine.wrap(BEAM)()
			coroutine.wrap(KillAll)()
			coroutine.wrap(Explosion)()
			coroutine.wrap(BEAMEND)()
			
			coroutine.wrap(FEBEAM)()
			
			coroutine.wrap(FEBEAM2)()
			
			coroutine.wrap(FEEXPLOSION)()
			
			coroutine.wrap(FEPORTAL)()
			end
			
			
			
			
			
			
			
		
		
		
		
		
		
		
		end)
end
