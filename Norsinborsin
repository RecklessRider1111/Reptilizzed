				workspace.resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", p, p, tbl)
			end
		end;
		function() _G.Voltaris = true
			while _G.Voltaris == true do
				wait()
				local p = game.Players.LocalPlayer.Character.Torso.Position
				local tbl = {
					["CanCollide"] = true,
