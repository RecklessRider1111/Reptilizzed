--Cannon Arm



function createshooty(part,start,endpos)
    local lib = require(game:GetService("Workspace").upsilonLibrary)
    
                        local dist = (start-endpos).magnitude
                        local properties = {
                            ["Material"] = Enum.Material.Neon,  
                            ["BrickColor"] = BrickColor.new("Really red"),
                            ["Reflectance"] = 0,
                            ["CanCollide"] = false,
                   ["Name"] = "Part",
                            ["Size"] = Vector3.new(0.5, 0.5, dist),
                            ["Parent"] = Workspace
                        }
                        
                    firesignal(game.Workspace.resources.RemoteEvent.OnClientEvent,"drawLaser",start,endpos,properties)	lib.FireOtherClients("drawLaser",start,endpos,properties)
                    end
                    local mouse = game.Players.LocalPlayer:GetMouse()
                    local ray = Instance.new("Part")
                    ray.Parent = Workspace
                    local tool = Instance.new("Tool",game.Players.LocalPlayer.Backpack)
                    tool.Name = "Beams!!"
                    tool.RequiresHandle = false
    local h = Instance.new("Part")
    h.Parent = tool
    h.Size = Vector3.new(1, 1, 1)
    h.Name = "Handle"				
    h.Transparency = 1				
                    local doing = false
    
                    tool.Activated:Connect(function()
    for i,v in pairs(game.Players:GetPlayers()) do
        
        workspace.resources.RemoteEvent:FireServer("becomeHostile");	--workspace.resources.RemoteEvent:FireServer("dealMeleeDamage", v.Character.Humanoid, 15);
    end
                    
                    
        
                            createshooty(ray, game.Players.LocalPlayer.Character["Arm Cannon"].ShootPart.Position, mouse.Hit.p)
                                
                        
                        
                        local plr = game.Players.LocalPlayer
    local mouse = plr:GetMouse()
    
    
    
        local t = mouse.Target
        local target = nil
        if t then
          for i, v in pairs(game.Players:GetPlayers()) do
            if v.Character and v.Character:FindFirstChild("Torso") then
                if t:IsDescendantOf(v.Character) then
                    target = v
                    break
                end
            end
          end
        end
        if target then --your code down here:
    
    
    local args = {
        [1] = "FireOtherClients",
        [2] = "drawLaser",
        [3] = Vector3.new(0, 0, 0),
        [4] = Vector3.new(0, 0, 0),
        [5] = {
            ["Name"] = "Head",
            ["Parent"] = target.Character,
            ["Material"] = Enum.Material.ForceField,
            ["CFrame"] = CFrame.new(-20.000213623046875, 5.490209102630615, 19.997617721557617) * CFrame.Angles(-0.004397252108901739, 0.0030667458195239305, -1.872209054454288e-07),
            ["Size"] = Vector3.new(5, 1, 1)
        }
    }
    
    workspace.resources.RemoteEvent:FireServer(unpack(args))
    
    local Player = game:GetService("Players").LocalPlayer
    local client = Player.PlayerGui.autoExec.client
    local DrawLaserF = nil
    
    for I, V in pairs(getgc()) do
        if type(V) == "function" and getfenv(V).script == client and debug.getinfo(V).name == "drawLaser" then
            DrawLaserF = V
            break 
        end
    end
    function DrawLaser(...)
       DrawLaserF(...)
       game:GetService("Workspace").resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", ...)
    end
    
        local Pos = target.Character.Torso.Position
        DrawLaser(Pos, Vector3.new(Pos.X, Pos.Y, Pos.Z), {
        ["CanCollide"] = false,
        ["Parent"] = workspace,
        ["Anchored"] = true,
        ["Name"] = "Part",
        ["CastShadow"] = false,
        ["Transparency"] = 0,
        ["RotVelocity"] = Vector3.new(0,0,0),
        ["Position"] = Pos,
        ["BrickColor"] = BrickColor.new("Really red"),
        ["Material"] = Enum.Material.Foil,
        ["Shape"] = Enum.PartType.Ball,
        ["Size"] = Vector3.new(8, 8, 8)
    })
    
    
    
    
        end
    
           
                        
                        
                        
                        
                        
                        
                        
                        
                    
                    end)
    
                    tool.Deactivated:Connect(function()
                        doing = false
                    end)















local plr = game.Players.LocalPlayer
	local mouse = plr:GetMouse()
	local tool = Instance.new("Tool", plr.Backpack)
	tool.Name = "Swing"
	tool.CanBeDropped = false
	tool.RequiresHandle = false
	tool.Activated:connect(function()
	    local Anim = Instance.new("Animation")
Anim.AnimationId = "rbxassetid://218504594"
local track = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Anim)
local FullSwingACTIVE = false

    FullSwingACTIVE = not FullSwingACTIVE
    if FullSwingACTIVE then
      
        while wait() do
         if track.IsPlaying == false then
            if FullSwingACTIVE then
                track:Play(.1, 1, 1)
            wait(999999999)
               track:Stop()
            end
         end
        end
   end
	    end)





