coroutine.wrap(MDJBW_fake_script)()
local function ZHOCGK_fake_script() -- RoboPlasmaDemon.LocalScript 
	local script = Instance.new('LocalScript', RoboPlasmaDemon)

	script.Parent.MouseButton1Down:connect(function()
		
		
		
		
		local plr = game.Players.LocalPlayer
	local mouse = plr:GetMouse()
	local tool = Instance.new("Tool", plr.Backpack)
	tool.Name = "Use Sword"
	tool.CanBeDropped = false
	tool.RequiresHandle = false
	tool.Activated:connect(function()
	    local Anim = Instance.new("Animation")
Anim.AnimationId = "rbxassetid://218504594"
local track = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Anim)
local FullSwingACTIVE = false

    FullSwingACTIVE = not FullSwingACTIVE
    if FullSwingACTIVE then
      
        while wait() do
         if track.IsPlaying == false then
            if FullSwingACTIVE then
                track:Play(.1, 1, 1)
            wait(999999999)
               track:Stop()
            end
         end
        end
   end
	    end)



