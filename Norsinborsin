		local function FECH() -- TextButton.LocalScript 
			local script = Instance.new('LocalScript', workspace)
	  
	   local z = ch.SoldierOfGod
	  
	  local chr = game.Players.LocalPlayer.Character
	  _G.light = true
	  while _G.light == true do
		  wait()
		 if chr.Humanoid.Health == 0 then
		
		  ch.SoldierOfGod:Destroy()
		 local args = {
			[1] = "reloadMe"
		}
		
		workspace.resources.RemoteEvent:FireServer(unpack(args))
		 else
	
	  
	  
		 local p = z["Left Arm"].Position
	  local tbl = {
		  ["CanCollide"] = true,
		
		  ["Parent"] = workspace,
		  ["Anchored"] = true,
		  ["Name"] = "Part",
