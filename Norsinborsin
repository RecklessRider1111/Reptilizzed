    MainSection:NewToggle("Lightning Aura Effect", "Creates Aura", function(state)
        if state then
          


local pt = Instance.new("Part")
pt.Parent = game.Players.LocalPlayer.Character
pt.Anchored = false
pt.CanCollide= false
pt.Name = "FIELD"
pt.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
pt.Shape = Enum.PartType.Ball
pt.Material = Enum.Material.Neon
pt.BrickColor = BrickColor.new("New Yeller")
pt.Massless = true
pt.Size = Vector3.new(6,6,6)
pt.Transparency = 0.8


local w = Instance.new("Weld")
w.Parent = pt
w.Part0 = game.Players.LocalPlayer.Character.HumanoidRootPart
w.Part1 = pt



_G.EFFECT = true
while _G.EFFECT == true do
   wait()

   if game.Players.LocalPlayer.TeamColor == BrickColor.new("Really black") then
    local args = {

        [1] = workspace:WaitForChild("spawns"):WaitForChild("bottom"),
        [2] = game:GetService("Players").LocalPlayer.Backpack.Sniperswat.Handle,
        [3] = Vector3.new(0, 0, -9),
        [4] = game.Players.LocalPlayer.Character.HumanoidRootPart.Position,
        [5] = game:GetService("Players").LocalPlayer.Backpack.Sniperswat.GunScript_Local.MuzzleEffect,
        [6] = game:GetService("Players").LocalPlayer.Backpack.Sniperswat.GunScript_Local.HitEffect,
        [7] = 186809250,
        [8] = {
        [1] = false
        },
        [9] = {
        
        [1] = 25,
        [2] = Vector3.new(10.25, 888.25, 100),
        [3] =BrickColor.new(11),
        [4] = 111.25,
        [5] = Enum.Material.SmoothPlastic,
        [6] = 111.25,
        
        },
        [10] = true,
        [11] = false
        }
        game:GetService("ReplicatedStorage"):WaitForChild("VisualizeBullet"):FireServer(unpack(args))

   else
local args = {

[1] = workspace:WaitForChild("spawns"):WaitForChild("bottom"),
[2] = game:GetService("Players").LocalPlayer.Backpack.Snipercrim.Handle,
[3] = Vector3.new(0, 0, -9),
[4] = game.Players.LocalPlayer.Character.HumanoidRootPart.Position,
[5] = game:GetService("Players").LocalPlayer.Backpack.Snipercrim.GunScript_Local.MuzzleEffect,
[6] = game:GetService("Players").LocalPlayer.Backpack.Snipercrim.GunScript_Local.HitEffect,
[7] = 186809250,
[8] = {
[1] = false
},
[9] = {

[1] = 25,
[2] = Vector3.new(10.25, 888.25, 100),
[3] =BrickColor.new(11),
[4] = 111.25,
[5] = Enum.Material.SmoothPlastic,
[6] = 111.25,

},
[10] = true,
[11] = false
}
game:GetService("ReplicatedStorage"):WaitForChild("VisualizeBullet"):FireServer(unpack(args))
end
end  
        else
            game.Players.LocalPlayer.Character.FIELD:Destroy()    
        
            _G.EFFECT = false
            while _G.EFFECT == true do
               wait()
            end
        end
    end)






