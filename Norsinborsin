Abillity3.MouseButton1Down:connect(function()

	---Fire Ability Tool---
	local plr = game.Players.LocalPlayer
	local mouse = plr:GetMouse()
	local tool = Instance.new("Tool", plr.Backpack)
	tool.Name = "Abillity 3"
	tool.CanBeDropped = false
	tool.RequiresHandle = false
	local Handle = Instance.new("Part")	
	Handle.Name = "Handle"
	Handle.Parent = game.Players.LocalPlayer.Backpack["Abillity 3"]
	Handle.Size = Vector3.new(1, 1, 1)
	Handle.Transparency = 1
	tool.Activated:connect(function()

		local t = mouse.Target
		local target = nil
		if t then
			for i, v in pairs(game.Players:GetPlayers()) do
				if v.Character and v.Character:FindFirstChild("Torso") then
					if t:IsDescendantOf(v.Character) then
						target = v
						break
					end
				end
			end
		end

		if target then --your code down here:
			local COLOR = BrickColor.new("Dark indigo")
			local Player = game:GetService("Players").LocalPlayer                        
			local client = Player.PlayerGui.autoExec.client
			local DrawLaserF = nil

			for I, V in pairs(getgc()) do
				if type(V) == "function" and getfenv(V).script == client and debug.getinfo(V).name == "drawLaser" then
					DrawLaserF = V
					break 
				end
			end
			function DrawLaser(...)
				DrawLaserF(...)
				game:GetService("Workspace").resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", ...)
			end
			local coo = game.Players.LocalPlayer.Character["Abillity 3"].Handle.Position
			local Pos = coo
			DrawLaser(Pos, Vector3.new(Pos.X, Pos.Y, Pos.Z), {

				["CanCollide"] = false,
				["Parent"] = workspace,
				["Anchored"] = false,
				["Name"] = "WL",
				["CastShadow"] = false,
				["Transparency"] = 0,
				["RotVelocity"] = Vector3.new(0,0,100),
				["Position"] = Pos,
				["BrickColor"] = COLOR,
				["Material"] = Enum.Material.Neon,
				["Shape"] = Enum.PartType.Block,
				["Size"] = Vector3.new(2, 2, 2),
				["Rotation"] = Vector3.new(0, 0, 90),
				["AssemblyLinearVelocity"] = Vector3.new(0, 10, 0)
			})
			wait(0.1)


			local Player = game:GetService("Players").LocalPlayer                        
			local client = Player.PlayerGui.autoExec.client
			local DrawLaserF = nil

			for I, V in pairs(getgc()) do
				if type(V) == "function" and getfenv(V).script == client and debug.getinfo(V).name == "drawLaser" then
					DrawLaserF = V
					break 
				end
			end
			function DrawLaser(...)
				DrawLaserF(...)
				game:GetService("Workspace").resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", ...)
			end

			local Pos = coo
			DrawLaser(Pos, Vector3.new(Pos.X, Pos.Y, Pos.Z), {

				["CanCollide"] = false,
				["Parent"] = workspace,
				["Anchored"] = false,
				["Name"] = "WL",
				["CastShadow"] = false,
				["Transparency"] = 0,
				["RotVelocity"] = Vector3.new(0,0,100),
				["Position"] = Pos,
				["BrickColor"] = COLOR,
				["Material"] = Enum.Material.Neon,
				["Shape"] = Enum.PartType.Block,
				["Size"] = Vector3.new(2, 2, 2),
				["Rotation"] = Vector3.new(0, 0, 90),
				["AssemblyLinearVelocity"] = Vector3.new(0, 10, 0)
			})
			wait(0.1)


			local Player = game:GetService("Players").LocalPlayer                        
			local client = Player.PlayerGui.autoExec.client
			local DrawLaserF = nil

			for I, V in pairs(getgc()) do
				if type(V) == "function" and getfenv(V).script == client and debug.getinfo(V).name == "drawLaser" then
					DrawLaserF = V
					break 
				end
			end
			function DrawLaser(...)
				DrawLaserF(...)
				game:GetService("Workspace").resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", ...)
			end

			local Pos = coo
			DrawLaser(Pos, Vector3.new(Pos.X, Pos.Y, Pos.Z), {

				["CanCollide"] = false,
				["Parent"] = workspace,
				["Anchored"] = false,
				["Name"] = "WL",
				["CastShadow"] = false,
				["Transparency"] = 0,
