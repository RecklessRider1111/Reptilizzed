Abillity1.Name = "Abillity1"
Abillity1.Parent = main
Abillity1.BackgroundColor3 = Color3.fromRGB(108, 0, 162)
Abillity1.Position = UDim2.new(0, 0, 0.779710174, 0)
Abillity1.Size = UDim2.new(0, 36, 0, 68)
Abillity1.Font = Enum.Font.SourceSans
Abillity1.Text = "1"
Abillity1.TextColor3 = Color3.fromRGB(0, 0, 0)
Abillity1.TextScaled = true
Abillity1.TextSize = 14.000
Abillity1.TextWrapped = true
Abillity1.MouseButton1Down:connect(function()
	local plr = game.Players.LocalPlayer
	local mouse = plr:GetMouse()
	local tool = Instance.new("Tool", plr.Backpack)
	tool.Name = "Abillity1"
	tool.CanBeDropped = false
	tool.RequiresHandle = false
	tool.Activated:connect(function()
		local Anim = Instance.new("Animation")
		Anim.AnimationId = "rbxassetid://204062532"
		local track = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Anim)
		local FullPunchACTIVE = false

		FullPunchACTIVE = not FullPunchACTIVE
