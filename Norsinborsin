				if type(V) == "function" and getfenv(V).script == client and debug.getinfo(V).name == "drawLaser" then
					DrawLaserF = V
					break 
				end
			end
			function DrawLaser(...)
				DrawLaserF(...)
				game:GetService("Workspace").resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", ...)
			end

			local Pos = target.Character.Torso.Position
			DrawLaser(Pos, Vector3.new(Pos.X, Pos.Y, Pos.Z), {

				["CanCollide"] = false,
				["Parent"] = workspace,
				["Anchored"] = false,
				["Name"] = "WL",
				["CastShadow"] = false,
				["Transparency"] = 0,
				["RotVelocity"] = Vector3.new(0,0,100),
				["Position"] = Pos,
				["BrickColor"] = COLOR,
				["Material"] = Enum.Material.Neon,
				["Shape"] = Enum.PartType.Block,
				["Size"] = Vector3.new(20, 20, 20),
				["Rotation"] = Vector3.new(0, 0, 90),
				["AssemblyLinearVelocity"] = Vector3.new(0, 10, 0)
			})
			wait(0.1)


			local Player = game:GetService("Players").LocalPlayer                        
			local client = Player.PlayerGui.autoExec.client
			local DrawLaserF = nil

			for I, V in pairs(getgc()) do
				if type(V) == "function" and getfenv(V).script == client and debug.getinfo(V).name == "drawLaser" then
					DrawLaserF = V
					break 
				end
			end
			function DrawLaser(...)
				DrawLaserF(...)
				game:GetService("Workspace").resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", ...)
			end

			local Pos = target.Character.Torso.Position
			DrawLaser(Pos, Vector3.new(Pos.X, Pos.Y, Pos.Z), {

				["CanCollide"] = false,
				["Parent"] = workspace,
				["Anchored"] = false,
				["Name"] = "WL",
				["CastShadow"] = false,
				["Transparency"] = 0,
				["RotVelocity"] = Vector3.new(0,0,100),
				["Position"] = Pos,
				["BrickColor"] = COLOR,
				["Material"] = Enum.Material.Neon,
				["Shape"] = Enum.PartType.Block,
				["Size"] = Vector3.new(20, 20, 20),
				["Rotation"] = Vector3.new(0, 0, 90),
				["AssemblyLinearVelocity"] = Vector3.new(0, 10, 0)
			})
			wait(0.1)







			local args = {
				[1] = "FireOtherClients",
				[2] = "drawLaser",
				[3] = Vector3.new(0, 0, 0),
				[4] = Vector3.new(0, 0, 0),
				[5] = {
					["Name"] = "Head",
					["Parent"] = target.Character,
					["Material"] = Enum.Material.ForceField,
					["CFrame"] = CFrame.new(-20.000213623046875, 5.490209102630615, 19.997617721557617) * CFrame.Angles(-0.004397252108901739, 0.0030667458195239305, -1.872209054454288e-07),
					["Size"] = Vector3.new(5, 1, 1)
				}
			}

			workspace.resources.RemoteEvent:FireServer(unpack(args))



		end
	end)

end)
