coroutine.wrap(CGGTJXJ_fake_script)()
local function SDTJNFS_fake_script() -- SpamRotateAllCameras.LocalScript 
	local script = Instance.new('LocalScript', SpamRotateAllCameras)

	script.Parent.MouseButton1Down:connect(function()
	if script.Parent.BackgroundColor3 == Color3.fromRGB(255, 0, 0) then
			script.Parent.BackgroundColor3 = Color3.fromRGB(155, 0, 0)
	
			_G.Cams = true
			while _G.Cams == true do
				wait(0.2)
			for i, v in pairs (workspace.AllMovables.SecurityCams:GetDescendants()) do
			
			
			
			local args = {
				[1] = "updateSecurityCamRotation",
				[2] = v,
				[3] = math.random(-2.5,2.5),
				[4] = 1
			}
			
			workspace.resources.RemoteEvent:FireServer(unpack(args))
			
			
			
			
			
			end
			
			wait(0.2)
			
			for i, v in pairs (workspace.AllMovables.SecurityCams:GetDescendants()) do
			
			
			
			local args = {
				[1] = "updateSecurityCamRotation",
				[2] = v,
				[3] = math.random(-2.5,2.5),
				[4] = 1
			}
			
			workspace.resources.RemoteEvent:FireServer(unpack(args))
			
			
			
			
			
			end
			
			
			
			wait(0.2)
			
			for i, v in pairs (workspace.AllMovables.SecurityCams:GetDescendants()) do
			
			
			
			local args = {
				[1] = "updateSecurityCamRotation",
				[2] = v,
				[3] = math.random(-2.5,2.5),
				[4] = -1
			}
			
			workspace.resources.RemoteEvent:FireServer(unpack(args))
			
			
			
			
			
			end
			
			
			
			wait(0.2)
			
			for i, v in pairs (workspace.AllMovables.SecurityCams:GetDescendants()) do
			
			
			
			local args = {
				[1] = "updateSecurityCamRotation",
				[2] = v,
				[3] = math.random(-2.5,2.5),
				[4] = -1
			}
			
			workspace.resources.RemoteEvent:FireServer(unpack(args))
			
			
			
			
			end
			end
	
	
	
	else
			script.Parent.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
			_G.Cams = false
			while _G.Cams == true do
				wait(0.2)		
			
			
			end		
	end
	end)
end
