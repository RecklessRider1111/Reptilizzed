            game:GetService("Players").LocalPlayer.Backpack.Riflecrim.GunScript_Server.InflictTarget:FireServer(unpack(args))
            
            
            
            
            
            local args = {
            [1] = game.Players:GetPlayers()[16].Character.Humanoid, 
            [2] = game.Players:GetPlayers()[16].Character.UpperTorso, 
            [3] = 1000.5,
            [4] = Vector3.new(0.829352080821991, -0.26517945528030396, -0.49178752303123474),
            [5] = 0,
            [6] = false, 
            [7] = false
            }
            game:GetService("Players").LocalPlayer.Backpack.Riflecrim.GunScript_Server.InflictTarget:FireServer(unpack(args))
            
            
            
            
            
            
            
        end
    end)





