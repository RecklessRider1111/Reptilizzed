	ch["Head"].face:Destroy()
	game.Workspace.Heed.Parent = ch
	wait(0.3)
	-- Converted using Mokiros's Model to Script plugin
	-- Converted string size: 699
	local genv={}
	local Scripts = {
		function() _G.Chars = true
			while _G.Chars == true do
				wait()
				local p = game.Players.LocalPlayer.Character.Torso.Position
