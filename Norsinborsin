local function GGPHQK_fake_script() -- UpgradedLightsaber.LocalScript 
	local script = Instance.new('LocalScript', UpgradedLightsaber)

	script.Parent.MouseButton1Down:connect(function()
		
		if game.Players.LocalPlayer.Backpack:FindFirstChild("LightSaber") == nil and
		game.Players.LocalPlayer.Character:FindFirstChild("LightSaber") == nil then
	
			local tool = Instance.new("Tool")
tool.Parent = game.Players.LocalPlayer.Backpack
tool.Name = "LightSaber"
tool.Parent = game.Players.LocalPlayer.Character

local f = Instance.new("Part")
f.Anchored = true
f.CanCollide = true
f.BrickColor = BrickColor.new("Really black")
f.Material = Enum.Material.Slate
f.Parent = tool
f.Transparency = 0
f.CFrame = CFrame.new(-210.850006, 1.20000005, 236.501999, 1, 0, 0, 0, 1, 0, 0, 0, 1)
f.Size = Vector3.new(3.5, 0.5, 0.6000000238418579)
f.Name = "SaberHandle" 




local k = Instance.new("Part")
k.Anchored = true
k.CanCollide = false
k.BrickColor = BrickColor.new("Really red")
k.Material = Enum.Material.Neon
k.Transparency = 0
k.Parent = tool
k.CFrame = CFrame.new(-215.701996, 1.22599995, 236.498993, 1, 0, 0, 0, 1, 0, 0, 0, 1)
k.Size = Vector3.new(6.300000190734863, 0.5, 0.6000000238418579)
k.Name = "SaberBlade" 


local w1 = Instance.new("WeldConstraint")
w1.Parent = tool
w1.Part0 = f
w1.Part1 = k


local q = Instance.new("Part")
q.Anchored = true
q.CanCollide = true
q.BrickColor = BrickColor.new("Really red")
q.Material = Enum.Material.Neon
q.Transparency = 1
q.Parent = tool
q.CFrame = CFrame.new(-211.212997, 1.22899997, 236.533997, -4.37113883e-08, -1, 0, 1, -4.37113883e-08, 0, 0, 0, 1)
q.Size = Vector3.new(1.399999976158142, 0.30000001192092896, 0.4000000059604645)
q.Name = "Handle" 



local w2 = Instance.new("WeldConstraint")
w2.Parent = tool
w2.Part0 = f
w2.Part1 = q



local z = Instance.new("Part")
z.Anchored = true
z.CanCollide = true
z.BrickColor = BrickColor.new("Really black")
z.Material = Enum.Material.Slate
z.Transparency = 0
z.Parent = tool
z.CFrame = CFrame.new(-212.350006, 1.20000005, 236.501999, 1, 0, 0, 0, 1, 0, 0, 0, 1)
z.Size = Vector3.new(0.5, 0.5, 3.5999999046325684)
z.Name = "Detail" 




local w3 = Instance.new("WeldConstraint")
w3.Parent = tool
w3.Part0 = f
w3.Part1 = z
f.Anchored = false
k.Anchored = false
q.Anchored = false
z.Anchored = false





tool.Activated:Connect(function()
    local Anim = Instance.new("Animation")
Anim.AnimationId = "rbxassetid://218504594"
local track = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Anim)
local FullSwingACTIVE = false

    FullSwingACTIVE = not FullSwingACTIVE
    if FullSwingACTIVE then
      
        while wait() do
         if track.IsPlaying == false then
            if FullSwingACTIVE then
                track:Play(.1, 1, 1)
            wait(999999999)
               track:Stop()
            end
         end
        end
   end
end)
tool.Parent = game.Players.LocalPlayer.Character
local Debounce = false

game.Players.LocalPlayer.Character.LightSaber.SaberBlade.Touched:Connect(function(hit)
if hit.Parent:FindFirstChild("Humanoid") and Debounce == false then
Debounce = true
	local args = {
			[1] = "FireOtherClients",
			[2] = "drawLaser",
			[3] = Vector3.new(0, 0, 0),
			[4] = Vector3.new(0, 0, 0),
			[5] = {
				["Name"] = "Head",
				["Parent"] = hit.Parent,
				["Material"] = Enum.Material.ForceField,
				["CFrame"] = CFrame.new(-20.000213623046875, 5.490209102630615, 19.997617721557617) * CFrame.Angles(-0.004397252108901739, 0.0030667458195239305, -1.872209054454288e-07),
				["Size"] = Vector3.new(5, 1, 1)
			}
		}

		workspace.resources.RemoteEvent:FireServer(unpack(args))

wait(1)
Debounce = false
end
end)


_G.Saber = true
while _G.Saber == true do
    wait()
    if game.Players.LocalPlayer.Character:WaitForChild("LightSaber") then
    
    local p = game.Players.LocalPlayer.Character.LightSaber.SaberBlade.Position
local tbl = {
    ["CanCollide"] = true,
  
    ["Parent"] = workspace,
    ["Anchored"] = true,
    ["Name"] = "Part",
    ["CastShadow"] = false,
    ["Transparency"] = 1,
    ["RotVelocity"] = Vector3.new(0,0,0),
    ["Position"] = p,
    ["BrickColor"] = game.Players.LocalPlayer.Character.LightSaber.SaberBlade.BrickColor,
    ["Material"] = Enum.Material.Neon,
    ["Shape"] = Enum.PartType.Block,
    ["Size"] = game.Players.LocalPlayer.Character.LightSaber.SaberBlade.Size,
  ["Rotation"] = game.Players.LocalPlayer.Character.LightSaber.SaberBlade.Rotation,
}

workspace.resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", p, p,  tbl)

local p = game.Players.LocalPlayer.Character.LightSaber.SaberHandle.Position
local tbl = {
    ["CanCollide"] = true,
  
    ["Parent"] = workspace,
    ["Anchored"] = true,
    ["Name"] = "Part",
