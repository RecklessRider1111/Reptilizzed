
      
	local word = "A black hole has been spotted"
	local lib = require(game:GetService("Workspace").upsilonLibrary)
	firesignal(game.Workspace.resources.RemoteEvent.OnClientEvent,"displayChoice",'returnToMenu',word)
	lib.FireOtherClients("displayChoice",'returnToMenu',word)
      
      wait(2)
      
      local word = "Make sure to take...#.#%%%..##%%%%%"
	local lib = require(game:GetService("Workspace").upsilonLibrary)
	firesignal(game.Workspace.resources.RemoteEvent.OnClientEvent,"displayChoice",'returnToMenu',word)
	lib.FireOtherClients("displayChoice",'returnToMenu',word)


    wait(5)
     
    
    
    local a = Instance.new("Model")
        a.Name = "BlackHole"
        a.Parent = workspace
    
    local an = true
      local am = false


local function Start() -- ClickFling.LocalScript 
    
       
        local b  =  Instance.new("Part")
        b.Size = Vector3.new(1750, 250, 1484)
        b.Parent = a
        b.Material = Enum.Material.ForceField 
        b.CFrame = CFrame.new(-128.380066, -1.5, -386.209351, 1, 0, 0, 0, 1, 0, 0, 0, 1)
        b.BrickColor = BrickColor.new("Dark stone grey")
        b.Transparency = 0
        b.CanCollide = false
        b.CanTouch = false
        b.Anchored = an
        b.Shape = Enum.PartType.Block
         b.Name = "SuckPart"
        b.AssemblyLinearVelocity = Vector3.new(0, 0, -99)
       
    game.Workspace:WaitForChild("BlackHole")
    game.Workspace.BlackHole:WaitForChild("SuckPart")
    local z = game.Workspace.BlackHole.SuckPart
    
    _G.BlackHole = true
    while _G.BlackHole == true do
        wait()
    
    if game.Workspace.BlackHole:FindFirstChild("SuckPart") then
    
        
        local p = z.Position
        local tbl = {
            ["CanCollide"] = true,
          
            ["Parent"] = workspace,
            ["Anchored"] = true,
            ["Name"] = "Part",
            ["CastShadow"] = false,
            ["Transparency"] = 1,
            ["RotVelocity"] = Vector3.new(0,0,0),
            ["Position"] = p,
            ["BrickColor"] = z.BrickColor,
            ["Material"] = Enum.Material.ForceField,
            ["Shape"] = Enum.PartType.Block,
            ["Size"] = z.Size,
          ["Rotation"] = z.Rotation,
       ["AssemblyLinearVelocity"] = z.AssemblyLinearVelocity
        }
        
        workspace.resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", p, p,  tbl)
          
    
    else
        _G.BlackHole = false
        while _G.BlackHole == true do
            wait()
    
    end
    end
    end
    end
    coroutine.wrap(Start)()
    
    
    

local function BlackHoleItself() -- ClickFling.LocalScript 

local c  =  Instance.new("Part")
c.Size = Vector3.new(2042, 2042, 2042)
c.Parent = a
c.Material = Enum.Material.SmoothPlastic 
c.CFrame = CFrame.new(-40.7982483, 123.5, -2038.271, 1, 0, 0, 0, 1, 0, 0, 0, 1)
c.BrickColor = BrickColor.new("Really black")
c.Transparency = 0
c.CanCollide = true
c.CanTouch = true
c.Anchored = an
c.Shape = Enum.PartType.Ball
 c.Name = "Hole"


 game.Workspace:WaitForChild("BlackHole")
 game.Workspace.BlackHole:WaitForChild("Hole")
 local z = game.Workspace.BlackHole.Hole
 
 _G.Hole = true
 while _G.Hole == true do
     wait()
 
 if game.Workspace.BlackHole:FindFirstChild("Hole") then
 
     
     local p = z.Position
     local tbl = {
         ["CanCollide"] = true,
       
         ["Parent"] = workspace,
         ["Anchored"] = true,
         ["Name"] = "Part",
         ["CastShadow"] = false,
         ["Transparency"] = 0,
         ["RotVelocity"] = Vector3.new(0,0,0),
         ["Position"] = p,
         ["BrickColor"] = z.BrickColor,
         ["Material"] = Enum.Material.SmoothPlastic,
         ["Shape"] = Enum.PartType.Ball,
         ["Size"] = z.Size,
       ["Rotation"] = z.Rotation,
    
     }
     
     workspace.resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", p, p,  tbl)
       
 
 else
     _G.Hole = false
     while _G.Hole == true do
         wait()
 
 end
 end
 end
 end
 


