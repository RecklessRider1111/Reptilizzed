          w.C0 = CFrame.new(0, -0.699999988, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1)


          local Debounce = false
          a.Blade.Touched:Connect(function(hit)
          if game.Players.LocalPlayer.Character:FindFirstChild("Swing") then
            if hit.Parent:FindFirstChild("Humanoid") and Debounce == false then
          
          
          
              local args = {
                      [1] = "FireOtherClients",
                      [2] = "drawLaser",
                      [3] = Vector3.new(0, 0, 0),
                      [4] = Vector3.new(0, 0, 0),
                      [5] = {
                          ["Name"] = "Head",
                          ["Parent"] = hit.Parent,
                          ["Material"] = Enum.Material.ForceField,
                          ["CFrame"] = CFrame.new(-20.000213623046875, 5.490209102630615, 19.997617721557617) * CFrame.Angles(-0.004397252108901739, 0.0030667458195239305, -1.872209054454288e-07),
                          ["Size"] = Vector3.new(5, 1, 1)
                      }
                  }
          
                  workspace.resources.RemoteEvent:FireServer(unpack(args))
                end
          end
          end)



local x = a
while task.wait(0.05) do


    local p = x["Part1"].Position
    local tbl = {
        ["CanCollide"] = true,
      
