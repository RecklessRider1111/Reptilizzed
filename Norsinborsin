coroutine.wrap(FE)()



local function Kill() 
    game.Workspace:WaitForChild("Sentry")
    game.Workspace.Sentry:WaitForChild("ShootPart")

    local part =  game.Workspace.Sentry.ShootPart

    function findPlayer()
        local distance = 25
        local target = nil
        for i, v in pairs(game.Workspace:GetChildren()) do
            local humanoid = v:FindFirstChild("Humanoid")
            local HumanoidRootPart = v:FindFirstChild("HumanoidRootPart")
            if humanoid and HumanoidRootPart and v ~= part then
                if (part.Position - HumanoidRootPart.Position).magnitude < distance then
                    distance = (part.Position - HumanoidRootPart.Position).magnitude
                    target = HumanoidRootPart
                    
                end
                
            end
        end
        return target
    end
    
        while wait(0.1) do
        local PlayerFound = findPlayer()
        if PlayerFound then
            if  PlayerFound.Parent:FindFirstChild("Torso") then
            if PlayerFound.Parent:FindFirstChild("ForceField") == nil  then
           
           
        
          
                
                print("found")
            
            
            function createshooty(part,start,endpos)
                local lib = require(game:GetService("Workspace").upsilonLibrary)
                
                                    local dist = (start-endpos).magnitude
                                    local properties = {
                                        ["Material"] = Enum.Material.Neon,  
                                        ["BrickColor"] = BrickColor.new("Really red"),
                                        ["Reflectance"] = 0,
                                        ["CanCollide"] = false,
                               ["Name"] = "zapChain",
                                        ["Size"] = Vector3.new(0.5, 0.5, dist),
                                        ["Parent"] = Workspace
                                    }
                                    
                                firesignal(game.Workspace.resources.RemoteEvent.OnClientEvent,"drawLaser",start,endpos,properties)	lib.FireOtherClients("drawLaser",start,endpos,properties)
                                end
            
                           
                                  
                                        createshooty(ray, game.Workspace.Sentry.ShootPart.Position,  PlayerFound.Parent.Torso.Position)
                                        wait()
                                  
                                
                
            
            
            local args = {
                [1] = "FireOtherClients",
                [2] = "drawLaser",
                [3] = Vector3.new(0, 0, 0),
                [4] = Vector3.new(0, 0, 0),
                [5] = {
                    ["Name"] = "Head",
                    ["Parent"] = PlayerFound.Parent,
                    ["Material"] = Enum.Material.ForceField,
                    ["CFrame"] = CFrame.new(-20.000213623046875, 5.490209102630615, 19.997617721557617) * CFrame.Angles(-0.004397252108901739, 0.0030667458195239305, -1.872209054454288e-07),
                    ["Size"] = Vector3.new(5, 1, 1)
                }
            }
    
            workspace.resources.RemoteEvent:FireServer(unpack(args))
     
            if PlayerFound.Parent.Humanoid.Health == 0 then 
                PlayerFound.Parent.Torso:Destroy()
            end
        else
            game.Workspace.Name = "Workspace"
        end
    end
end    
    end
end


coroutine.wrap(Kill)()




local function TP() 
game.Workspace:WaitForChild("Sentry")



wait(2)


wait(0.5)


local j  =  Instance.new("Part")
j.Size = Vector3.new(1, 1, 1)
j.Parent = game.Players.LocalPlayer.Character
j.Material = Enum.Material.Neon 
j.CFrame = CFrame.new(24.5227127, 3, 31.9361935, 1, 0, 0, 0, 1, 0, 0, 0, 1)
j.BrickColor = BrickColor.new("Neon orange")
