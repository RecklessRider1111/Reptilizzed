local function ASIBNPC_fake_script() -- ClickBuildTool.LocalScript 
	local script = Instance.new('LocalScript', ClickBuildTool)

	script.Parent.MouseButton1Down:connect(function()
	
	if game.Players.LocalPlayer.Backpack:FindFirstChild("Build") == nil and
	game.Players.LocalPlayer.Character:FindFirstChild("Build") == nil then
	local function Destroy() 
			while task.wait() do
			 
			 if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
			 game.Workspace.THEGROUP:Destroy()
			 
			 end
			 end
			 end
			 coroutine.wrap(Destroy)()
			 
					 
		  if game.Workspace:FindFirstChild("THEGROUP") then
		  game.Workspace.THEGROUP:Destroy()
		  end
		   local m = Instance.new("Model")
					  m.Parent = workspace
					  m.Name = "THEGROUP"
					  
		  local plr = game.Players.LocalPlayer
			  local mouse = plr:GetMouse()
			  local tool = Instance.new("Tool", plr.Backpack)
			  tool.Name = "Build"
			  tool.CanBeDropped = false
			  tool.RequiresHandle = false
			  tool.Activated:connect(function()
		  local pos = mouse.Hit
					  pos = Vector3.new(pos.X,pos.Y,pos.Z)
		  
					  
					  
					  
					  local J = Instance.new("Part")
					 
					  J.CanTouch = true
					  J.Anchored = true
					  J.Transparency = 0
					  J.CanCollide = true
					  J.Parent = m
					  J.Position = pos
					  J.Name = "BuildPart"
					  J.Material = Enum.Material.Slate
					  J.BrickColor = BrickColor.new("Medium stone grey")
		  J.Shape = Enum.PartType.Block
		  J.Size = Vector3.new(10, 12, 10)
		  
					  local args = {
						  [1] = "FireOtherClients",
						  [2] = "drawLaser",
						  [3] = Vector3.new(0, 0, 0) ,
						  [4] = Vector3.new(0, 0, 0),
						  [5] = {
							  ["CFrame"] = J.CFrame,
							  ["Transparency"] = 0,
							  ["BrickColor"] = J.BrickColor,
							  ["CanCollide"] = true,
							  ["farqhhshshshldldjddj"] = true,
							  ["Anchored"] = true,
							  ["Parent"] = game.Players.LocalPlayer.Character,
							  ["Material"] = J.Material,
							  ["Size"] = J.Size,
							  ["Shape"] = J.Shape
					  
						  }
					  }
					  workspace.resources.RemoteEvent:FireServer(unpack(args))
					  
					  
					  
					  local p = game.Players.LocalPlayer.Character.Torso.Position
					  local tbl = {
						  ["CanCollide"] = true,
						  ["Parent"] = game.Players.LocalPlayer.Character,
						  ["Anchored"] = true,
						  ["Name"] = "Part",
						  ["CastShadow"] = false,
						  ["Transparency"] = 0,
						  ["RotVelocity"] = Vector3.new(0,0,0),
						  ["CFrame"] = J.CFrame,
						  ["BrickColor"] = J.BrickColor,
						  ["Material"] = J.Material,
						  ["Shape"] = J.Shape,
						  ["Size"] = J.Size,
						  ["xyzk9zxvc"] = true 
					  }
					  
					  workspace.resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", p, p, tbl)
					  
		  
			  end)
	
	
			end
	
	
	
	
	
	end)
end
