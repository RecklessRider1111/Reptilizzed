local function FECannon() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', workspace)

local COLOR = BrickColor.new("Really black")
local COLOR2 = BrickColor.new("Really red")

    local a = Instance.new("Model")
a.Name = "Arm Cannon"
a.Parent = game.Players.LocalPlayer.Character



local b  =  Instance.new("Part")
    b.Size = Vector3.new(0.7099000215530396, 0.6800000071525574, 0.7448999881744385)
    b.Parent = a
    b.Material = Enum.Material.SmoothPlastic 
    b.CFrame = CFrame.new(2.48009992, 2.34929991, 23.2623997, -1, 0, -8.74227766e-08, 0, 1, 0, 8.74227766e-08, 0, -1)
  
    b.BrickColor = COLOR
    b.Transparency = 1
    b.CanCollide = false
    b.CanTouch = false
    b.Anchored = an
    b.Shape = Enum.PartType.Block
     b.Name = "Handle"


    
     local c  =  Instance.new("Part")
     c.Size = Vector3.new(0.4350000023841858, 3.240000009536743, 1.0999000072479248)
     c.Parent = a
     c.Material = Enum.Material.SmoothPlastic 
     c.CFrame = CFrame.new(2.87759995, 2.47429991, 23.2549, 0.99619472, 0.0871557444, 0, -0.0871557444, 0.99619472, 0, 0, 0, 1)
     c.BrickColor = COLOR
