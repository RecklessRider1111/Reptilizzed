local function PSRCTBM_fake_script() -- ClickLiquify.LocalScript 
	local script = Instance.new('LocalScript', ClickLiquify)

	script.Parent.MouseButton1Down:connect(function()
		
		if game.Players.LocalPlayer.Character:FindFirstChild("Click Liquify") == nil and
		game.Players.LocalPlayer.Backpack:FindFirstChild("Click Liquify") == nil then
		
			local plr = game.Players.LocalPlayer
			local mouse = plr:GetMouse()
			local tool = Instance.new("Tool", plr.Backpack)
			tool.Name = "Click Liquify"
			tool.CanBeDropped = false
			tool.RequiresHandle = false
			
			tool.Activated:connect(function()
				local t = mouse.Target
				local target = nil
				if t then
				  for i, v in pairs(game.Players:GetPlayers()) do
					if v.Character and v.Character:FindFirstChild("Torso") then
						if t:IsDescendantOf(v.Character) then
							target = v
							break
						end
					end
				  end
				end
				if target then --your code down here:
			
			
			local args = {
				[1] = "FireOtherClients",
				[2] = "drawLaser",
				[3] = Vector3.new(0, 0, 0),
				[4] = Vector3.new(0, 0, 0),
