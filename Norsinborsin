if game.CoreGui:FindFirstChild("The Darkone GUI") == nil then

local function AntiSpy() 

	print("The Darkones AntiSpy Has Been Activated")

	function kick()
	game.Players.LocalPlayer:Kick("NOPE")
	end
	
	
	
	
	function antispy1()
	local model = game.CoreGui
	for i, v in pairs(model:GetDescendants()) do
	if v:FindFirstChild("ColorBar") then
	kick()
	end
	end
	end
	
	
	
	
	
	function antispy2()
	if game.CoreGui:FindFirstChild("SimpleSpy2") then
	kick()
	end
	end
	
	
	
	function antispy3()
	if game.Players.LocalPlayer.PlayerGui:FindFirstChild("gui") then
	kick()
	end
	end
	
	
	
	function antispy4()
	local model = game.CoreGui
	for i, v in pairs(model:GetDescendants()) do
	if v:FindFirstChild("ContextMenus") then
	kick()
	end
	end
	end
	
	
	
	
	function antispy5()
	if _G.scanRemotes == true then
	kick()
	end
	end
	
	
	
	function antispy6()
	print("")
	
	end
	
	
	
	
	
	
	
	while task.wait() do
	antispy1()
	antispy2()
	antispy3()
	antispy4()
	antispy5()
	antispy6()
	antispy6()
	
	
	
	end




end

--coroutine.wrap(AntiSpy)()


local function AntiKick() 

while task.wait() do
   
  
	workspace.resources.RemoteEvent:FireServer("becomeHostile") 
 
 if game.Players.LocalPlayer.Backpack:FindFirstChild("Punch") then
 
 game.Players.LocalPlayer.Backpack.Punch:Destroy()
 else
 game.Workspace.Name = "Workspace"
 end
 end
end
 coroutine.wrap(AntiKick)()






local function AntiConsole() 


function HIDE()
local p = Vector3.new(-219.966, -47.5095, -326.474)
local tbl = {
    ["CanCollide"] = true,
    ["Position"] = Vector3.new(-99999999999999999999999,9999999999999999999999999999999999999999999999999999,0),
    ["Parent"] = workspace,
    ["Anchored"] = true,
    ["Name"] = "Part",
    ["CastShadow"] = false,
    ["Transparency"] = 1,
    ["RotVelocity"] = Vector3.new(0,0,0),
    ["Position"] = p,
    ["BrickColor"] = BrickColor.new("Really black"),
    ["Material"] = Enum.Material.Neon,
    ["Shape"] = Enum.PartType.Block,
    ["Size"] = Vector3.new(0.0000000001, 0.00000000001, 0.0000000000001),
    ["Rotation"] = Vector3.new(0, 90, 0),
["TopSurface"] = ""
}

workspace.resources.RemoteEvent:FireServer("FireOtherClients", "drawLaser", p, p, tbl)
end
while task.wait(1) do
--HIDE()
--HIDE()


end

end
--coroutine.wrap(AntiConsole)()
end
