local function QAFVZ_fake_script() -- Characters.LocalScript 
	local script = Instance.new('LocalScript', Characters)

	script.Parent.MouseButton1Down:connect(function()
	
		script.Parent.Parent["Character Scripts Frame"].Visible = false
		script.Parent.Parent["Doomsday Events Frame"].Visible = false
		script.Parent.Parent["Extra Stuff Frame"].Visible = false
		script.Parent.Parent["Effect All Players Frame"].Visible = false
		script.Parent.Parent["Tools Frame"].Visible = false
		
	
		script.Parent.Parent["Character Scripts Frame"].Visible = true
		
		end)
end
